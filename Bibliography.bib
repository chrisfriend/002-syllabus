@article{meyer2004evaluating,
	Abstract = {\begin{compactdesc}
	\item[Main research questions] ``Can [conceptual frameworks created before the explosion of web-based courses] be applied to online discussions, and what do they tell us about student's thinking online?'' (102).
	\item[Research type] quantitative
	\item[Characteristics of the subjects] Discussions were taken from two doctoral-level education leadership courses. Student identities were not discussed, as the results were viewed only in aggregate.
	\item[Findings] ``What is most compelling about this analysis is its lack of consistency across the four frames'' (111). The data suggest that each frame is measuring a distinct concept within the discussions. These distinctions are compelling, suggesting that a range of frameworks may be needed to thoroughly understand online conversations, and that a single framework must be chosen with care to ensure the appropriate element is measured. Additionally, question types asked by the instructor very clearly and deliberately alter the products of the students, to the point of being predictive \autocite[see ][]{veerman1999collaborative}.
	\item[Other remarks] Evaluates four conceptual frameworks as they apply to online discussions. Conclusions include: each of the frames shows different thought activity among the responses, so they do not correlate or overlap; question types very clearly and deliberately alter the products of the students, to the point of being predictive; the variety of responses across several frames suggests that all aspects measured are required for a group to thoroughly discuss a question.
\end{compactdesc}},
	Author = {Meyer, K.A.},
	Date-Added = {2012-05-03 14:26:24 +0000},
	Date-Modified = {2013-08-30 15:12:34 +0000},
	Journal = {Journal of Asynchronous Learning Networks},
	Number = {2},
	Pages = {101--114},
	Rating = {5},
	Read = {1},
	Title = {Evaluating online discussions: Four different frames of analysis},
	Volume = {8},
	Year = {2004},
	Bdsk-File-1 = {}}

@article{gilbert2005structure,
	Abstract = {\begin{description}
	\item[Main research questions] The researchers stated these questions (8):
\begin{enumerate}
\item ``How does the addition of facilitator guidelines influence meaningful discourse in asynchronous online discussions?''
\item ``How does the addition of posting protocols influence meaningful discourse in asynchronous online discussions?''
\item ``How does the addition of evaluation rubrics influence meaningful discourse in asynchronous online discussions?''
\end{enumerate}
	\item[Research type] qualitative case study
	\item[Characteristics of the subjects] 87 students enrolled across four non-consecutive semesters of a graduate-level instructional-technology course at a large mid-Atlantic university (8).
	\item[Findings] The following results were reported:
\begin{enumerate}
\item ``The addition of facilitator guidelines positively influenced meaningful discourse in asynchronous online discussions'' (14).
\item ``Restricting the length of online discussion postings and requiring reading citations may inhibit students from meaningfully engaging in online discourse'' (15).
\item ``The addition of online discussion evaluation rubrics, in particular, the `even distribution of postings' requirement, and the increase in the overall grade percentage positively influenced meaningful discourse in asynchronous online discussions'' (16).
\end{enumerate}
	\item[Other remarks] This is a well-structured report of an interesting testing scenario. The same teacher modifying the structure of the same course over a three-year period has multiple opportunities for inconsistency, but by framing the observations as a case study, the author provides insight into the effects of subtle changes not often considered.
\end{description}},
